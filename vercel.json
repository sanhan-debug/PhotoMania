{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/users/(.*)",
        "dest": "/us"
      },
      {
        "src": "/photos/(.*)",
        "dest": "/api/photos/$1"
      },
      {
        "src": "/feedback/(.*)",
        "dest": "/api/feedback/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ]
  }
  