{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/*",
            "dest": "/app.js"
        },
        {
            "src": "/about",
            "dest": "/Routers/pageRouters.js"
        },
        {
            "src": "/photos",
            "dest": "/Routers/pageRouters.js"
        },
        {
            "src": "/users/login",
            "dest": "/Routers/usersRoute.js"
        }
    ]
}